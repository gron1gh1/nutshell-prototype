{
  "name": "electron_example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/fluent-ffmpeg": "^2.1.17",
    "@types/jest": "^26.0.15",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "antd": "^4.15.3",
    "electron-is-dev": "^2.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.2.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "debug": "yarn tsc electron.ts && concurrently \"cross-env BROWSER=none react-scripts start\" \"wait-on http://localhost:3000 && electron --inspect=5858 .\"",
    "start": "yarn tsc electron.ts && concurrently \"cross-env BROWSER=none react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build && electron-builder",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "release": "react-scripts build && electron-builder --publish=always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "electron": "^12.0.5",
    "electron-builder": "^22.10.5",
    "wait-on": "^5.3.0"
  },
  "main": "electron.js"
}
